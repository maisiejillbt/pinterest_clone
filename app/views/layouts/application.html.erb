<!DOCTYPE html>
<html>
  <head>
    <title>Twintrest</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag 'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <% if current_user%>
      <script>
        window.current_user = { 
          "id": <%= current_user.id%>,
          "email": '<%= current_user.email%>',
          "username": '<%= current_user.username%>',
        };
      </script>
    <% end %>

    <script>
      window.main_logo ="<%= image_url("twintrest_logo.png") %>"
    </script>


  </head>
  <body>
    <%= yield %>
  </body>
</html>
